<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when button is pressed -->
    <item android:state_pressed="true" android:drawable="@drawable/inset_background" />

    <!-- State when button is focused -->
    <item android:state_focused="true" android:drawable="@drawable/inset_background" />

    <!-- Default state -->
    <item android:drawable="@android:color/transparent" />
</selector>
